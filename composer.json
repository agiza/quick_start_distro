{
  "name": "drupalcoders/quick_start_distro",
  "description": "Project template for Quick Start distribution.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
      "platform": {
          "php": "5.6"
      }
  },  
  "authors": [
    {
      "name": "lexsoft",
      "homepage": "http://drupalcoders.org/",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/quick_start",
    "source": "http://cgit.drupalcode.org/quick_start"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "1.4.0",
    "cweagans/composer-patches": "1.6.4",
    "drupal-composer/drupal-scaffold": "2.4.0",
    "drupal/drupal-library-installer-plugin": "0.3",
    "drupalcoders/quick_start": "dev-8.x-1.x"
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "extra": {
    "installer-paths": {
      "core": ["drupal/core"],
      "profiles/{$name}": ["type:drupal-profile"],
      "modules/contrib/{$name}": ["type:drupal-module"],
      "themes/contrib/{$name}": ["type:drupal-theme"],
      "libraries/{$name}": ["type:drupal-library"],
      "modules/custom/{$name}": ["type:drupal-custom-module"],
      "themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true
  }
}
