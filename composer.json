{
  "name": "drupalcoders/quick_start_distro",
  "description": "Project template for Quick Start distribution.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
      "platform": {
          "php": "5.6"
      }
  },  
  "authors": [
    {
      "name": "lexsoft",
      "homepage": "http://drupalcoders.org/",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/quick_start",
    "source": "http://cgit.drupalcode.org/quick_start"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
      "name": "enyo/dropzone",
      "version": "v4.3.0",
      "type": "drupal-library",
      "dist": {
        "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
        "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
      "name": "dinbror/blazy",
      "version": "1.8.2",
      "type": "drupal-library",
      "dist": {
        "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
        "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
      "name": "kenwheeler/slick",
      "version": "1.6.0",
      "type": "drupal-library",
      "dist": {
        "url": "https://github.com/kenwheeler/slick/archive/1.6.0.zip",
        "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
      "name": "ajaxorg/ace-builds",
      "version": "v1.2.6",
      "type": "drupal-library",
      "dist": {
        "url": "https://github.com/ajaxorg/ace-builds/archive/v1.2.6.zip",
        "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
      "name": "jackmoore/colorbox",
      "version": "1.6.4",
      "type": "drupal-library",
      "dist": {
        "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
        "type": "zip"
        }
      }
    }               
  ],
  "require": {
    "composer/installers": "1.4.0",
    "cweagans/composer-patches": "1.6.4",
    "drupalcoders/quick_start": "1.0-beta02"
  },
  "extra": {
    "installer-paths": {
      "core": ["drupal/core"],
      "profiles/{$name}": ["type:drupal-profile"],
      "modules/contrib/{$name}": ["type:drupal-module"],
      "themes/contrib/{$name}": ["type:drupal-theme"],
      "libraries/{$name}": ["type:drupal-library"],
      "modules/custom/{$name}": ["type:drupal-custom-module"],
      "themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true
  }
}
